{
    "files.exclude": {
        "_site": true,
        ".sass-cache": true,
        "vendor/bundle": true,
        ".gemini": true,
        "_includes/navigation.html": true,
        "assets/css/style.css": true
    },
    "search.exclude": {
        "_site": true,
        ".sass-cache": true,
        "vendor/bundle": true,
        ".gemini": true,
        "_includes/navigation.html": true,
        "assets/css/style.css": true
    },
    "editor.formatOnSave": true,
    "files.associations": {
        "search.json": "liquid"
    },
    "terminal.integrated.confirmationRequests.never": [
        "run",
        "kill",
        "clear",
        "curl",
        "iter",
        "wget",
        "ps",
        "grep",
        "git",
        "npm",
        "yarn",
        "bundle",
        "jekyll",
        "python",
        "node",
        "ruby",
        "brew",
        "pip",
        "pip3",
        "pnpm",
        "deno",
        "cargo",
        "go",
        "make",
        "docker",
        "kubectl",
        "ssh",
        "scp",
        "rsync",
        "sed",
        "awk",
        "find",
        "tar",
        "zip",
        "unzip",
        "osascript",
        "open",
        "*"
    ],
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.showExitAlert": false,
    "window.confirmBeforeClose": "never",
    "git.confirmSync": false,
    "git.confirmForcePush": false,
    "git.enableSmartCommit": true,
    "security.workspace.trust.enabled": false,
    "security.workspace.trust.untrustedFiles": "open"
}